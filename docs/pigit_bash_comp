#!/usr/env bash

_pigit_completion(){
  if [[ "${COMP_CWORD}" == "1" ]];then
      COMP_WORD="cF --commit bd R -h pc -r Rx Rs wm Rl -c ws pa push f cO l1 mF cm add pull user c ld sd --issue wM Si fc savepd --complete wD cr pt --create-ignore --count --dry-run cd --create-config wC ir pA Ra rename --print CO --simple lc Ru -v wc Cl iA clear bm fu pp b cmd -i l SD sl -t ca cR Rp Ce fm lr t --show-commands ls --types lv s Ct --path RS wX Sd iu open wR mt mC fr bL Co ta Ca -p -s iR sD cs rm iD Sc ue --shell --reverse -f CT ia cf email bc wd ll mv iX co ix p pF wx bl cA repo Su fetch --information sp tx fb --config bS m id bs --version pf --show-part-command wr --help mS Rm fC bM --report wS ma"
      COMPREPLY=($(compgen -W "$COMP_WORD" -- ${COMP_WORDS[${COMP_CWORD}]}))
  fi
}

complete -F _pigit_completion pigit
