#!/usr/env bash

_pigit_completion(){
  if [[ "${COMP_CWORD}" == "1" ]];then
      COMP_WORD="fu cO sD wr iu l1 --show-commands p fetch mv Si cs pull ls -i fb t sd --print mS --commit Rl mt --types ld iA RS -v mC --report open ix --reverse bS Su --version -c --config cm ll --simple l pp iD --complete cd fc savepd SD wS Rm wD wR pc Sc wx iR mF CO ir sp bL add Ct -s cf ia -f bM Ru --dry-run Rs rename push bl cF pF cr wc --path repo ca wX --help -h CT pt fr bc iX rm lv Rp --count co tx Co bm c bd ue id fC -t wd wM --shell fm -r Cl --information f ws ta Ra cmd lc --create-config Ca clear m user pA b ma --show-part-command lr wm s pf cR bs Ce email wC cA Rx --create-ignore R sl --issue Sd pa -p"
      COMPREPLY=($(compgen -W "$COMP_WORD" -- ${COMP_WORDS[${COMP_CWORD}]}))
  fi
}

complete -F _pigit_completion pigit
